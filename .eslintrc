{
  "extends": [
    "@lukemorales"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "arrowParens": "avoid"
      }
    ],
    "no-empty-pattern": "off",
    "jsx-a11y/media-has-caption": "off",
    "radar/no-duplicate-string": "off"
  }
}
